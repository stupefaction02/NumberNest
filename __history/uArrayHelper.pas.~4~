unit uArrayHelper;

interface

type
   TArrayHelper = class
   private

   public
      class function CreateRandom(Length: Int64) : TArray<Integer>;
   end;

implementation

class function TArrayHelper.CreateRandom(Length: Int64): TArray<Integer>;
var
  arr: TArray<Integer>;
  i: Integer;
begin
    SetLength(arr, Length);

    for i := 0 to Length - 1 do
      begin
        arr[i] := 10 + Random(100);
      end;

    Result := arr;
end;

end.
